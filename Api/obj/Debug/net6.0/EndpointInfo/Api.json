  "paths": {
    "/api/User/login": {
      "post": {
        "tags": [
          "User"
        ],
        "requestBody": {
          "content": {
            "application/json": {
              "schema": {
                "$ref": "#/components/schemas/Userlogin"
              }
            },
            "text/json": {
              "schema": {
                "$ref": "#/components/schemas/Userlogin"
              }
            },
            "application/*+json": {
              "schema": {
                "$ref": "#/components/schemas/Userlogin"
              }
            }
          }
        },
        "responses": {
          "200": {
            "description": "Success"
          }
        }
      }
    },
