                  "Firstname": {
                    "type": "string"
                  },
                  "Middlename": {
                    "type": "string"
                  },
                  "Lastname": {
                    "type": "string"
                  },
                  "Role": {
                    "type": "string"
                  },
                  "Username": {
                    "type": "string"
                  },
                  "Email": {
                    "type": "string"
                  },
                  "Password": {
                    "type": "string"
                  },
                  "Status": {
                    "type": "string"
                  },
                  "Avatar": {
                    "type": "string",
                    "format": "binary"
                  }
                }
              },
              "encoding": {
                "Firstname": {
                  "style": "form"
                },
                "Middlename": {
                  "style": "form"
                },
                "Lastname": {
                  "style": "form"
                },
                "Role": {
                  "style": "form"
                },
                "Username": {
                  "style": "form"
                },
                "Email": {
                  "style": "form"
                },
                "Password": {
                  "style": "form"
                },
                "Status": {
                  "style": "form"
                },
                "Avatar": {
                  "style": "form"
                }
              }
            }
          }
        },
        "responses": {
          "200": {
            "description": "Success"
          }
        }
      }
    },
    "/api/User/{id}": {
      "get": {
        "tags": [
          "User"
        ],
        "parameters": [
          {
            "name": "id",
            "in": "path",
            "required": true,
            "schema": {
              "type": "integer",
              "format": "int32"
            }
          }
        ],
        "responses": {
          "200": {
            "description": "Success"
          }
        }
      }
    },
    "/api/User/user-detail": {
      "get": {
        "tags": [
          "User"
        ],
        "responses": {
          "200": {
            "description": "Success",
